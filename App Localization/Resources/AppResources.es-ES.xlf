<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en-US" target-language="es-ES" original="AppResources.resx" tool-id="MultilingualAppToolkit" product-name="App Localization" product-version="1.0.0.0" build-num="0">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="3.1.1250.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="Resx">
        <trans-unit id="Resx/ResourceFlowDirection" translate="yes" xml:space="preserve">
          <source>LeftToRight</source>
          <target state="translated">LeftToRight</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Controls the FlowDirection for all elements in the RootFrame. Set to the traditional direction of this resource file's language</note>
          </trans-unit>
        <trans-unit id="Resx/ResourceLanguage" translate="yes" xml:space="preserve">
          <source>en-US</source>
          <target state="translated">es-ES</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Controls the Language and ensures that the font for all elements in the RootFrame aligns with the app's language. Set to the language code of this resource file's language.</note>
          </trans-unit>
        <trans-unit id="Resx/ApplicationTitle" translate="yes" xml:space="preserve">
          <source>App Localization</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Localización de la aplicación</target>
        </trans-unit>
        <trans-unit id="Resx/AppBarButtonText" translate="yes" xml:space="preserve">
          <source>add</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Agregar</target>
        </trans-unit>
        <trans-unit id="Resx/AppBarMenuItemText" translate="yes" xml:space="preserve">
          <source>Menu Item</source>
          <target state="translated" state-qualifier="tm-suggestion">Plato de menú</target>
        </trans-unit>
        <trans-unit id="Resx/cancel" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="translated" state-qualifier="tm-suggestion">Cancel</target>
        </trans-unit>
        <trans-unit id="Resx/checkEligibility" translate="yes" xml:space="preserve">
          <source>Check</source>
          <target state="translated" state-qualifier="tm-suggestion">Activar</target>
        </trans-unit>
        <trans-unit id="Resx/displayMessage" translate="yes" xml:space="preserve">
          <source>This sample assumes that people above 18 years of age are eligible to vote.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">En este ejemplo se supone que las personas mayores de 18 años son elegibles para votar.</target>
        </trans-unit>
        <trans-unit id="Resx/mainpagetitle" translate="yes" xml:space="preserve">
          <source>localization</source>
          <target state="translated" state-qualifier="tm-suggestion">localización</target>
        </trans-unit>
        <trans-unit id="Resx/noMessage" translate="yes" xml:space="preserve">
          <source>You are not eligible to vote.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">No cumples los requisitos votar.</target>
        </trans-unit>
        <trans-unit id="Resx/okay" translate="yes" xml:space="preserve">
          <source>OK</source>
          <target state="translated" state-qualifier="tm-suggestion">Aceptar</target>
        </trans-unit>
        <trans-unit id="Resx/userMessage" translate="yes" xml:space="preserve">
          <source>Enter your age</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Escriba su edad</target>
        </trans-unit>
        <trans-unit id="Resx/yesMessage" translate="yes" xml:space="preserve">
          <source>You are eligible to vote.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Tiene derecho a voto.</target>
        </trans-unit>
        <trans-unit id="Resx/validAge" translate="yes" xml:space="preserve">
          <source>Enter a valid age</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Escriba una duración válida</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>